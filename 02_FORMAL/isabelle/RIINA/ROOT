(*
 * RIINA Formal Proofs - Isabelle/HOL Port
 *
 * Multi-prover verification port from Coq 8.20.1 originals.
 * This provides independent verification of RIINA's core theorems.
 *
 * Reference: 02_FORMAL/coq/ (authoritative Coq proofs)
 *
 * Mode: ULTRA KIASU | ZERO TRUST | ABSOLUTE FIDELITY
 *)

session RIINA = HOL +
  description "RIINA: Rigorous Immutable Invariant, No Assumptions"
  options [quick_and_dirty = false, document = pdf]
  sessions
    "HOL-Library"
  theories
    Syntax
    Semantics
    Typing
    Progress
    Preservation
    TypeSafety
    EffectAlgebra
    EffectSystem
    EffectGate
    NonInterference
  document_files
    "root.tex"

