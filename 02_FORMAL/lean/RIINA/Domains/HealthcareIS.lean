  interaction_alert_shown di = true

/-- dose_in_range (matches Coq: Definition dose_in_range) -/
def dose_in_range (dc : DoseCheck) : Prop :=
  check_dose dc >= min_safe_dose dc /\
  check_dose dc <= max_safe_dose dc /\
  within_range dc = true

